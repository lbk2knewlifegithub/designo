{
  "name": "lbk",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "product-feedbacks:dev": "rm -rf dist && nx serve product-feedbacks -- --port=4200 --proxy-config proxy.conf.json",
    "product-feedbacks:build": "rm -rf dist && nx build product-feedbacks",
    "product-feedbacks:stage": "rm -rf dist && nx build product-feedbacks --configuration=staging",
    "product-feedbacks:sourceMap": "rm -rf dist && nx build product-feedbacks --sourceMap",
    "portfolio:dev": "rm -rf dist && nx serve portfolio -- --port=4201 --proxy-config proxy.conf.json",
    "portfolio:build": "rm -rf dist && nx build portfolio --base-href='https://lbk2knewlifegithub.github.io/lbk/'",
    "portfolio:sourceMap": "rm -rf dist && nx build portfolio --sourceMap",
    "audiophile:dev": "rm -rf dist && nx serve audiophile -- --port=4202 --proxy-config proxy.conf.json",
    "audiophile:build": "rm -rf dist && nx build audiophile",
    "audiophile:sourceMap": "rm -rf dist && nx build audiophile --sourceMap",
    "invoice-app:dev": "rm -rf dist && nx serve invoice-app -- --port=4203 --proxy-config proxy.conf.json",
    "invoice-app:build": "rm -rf dist && nx build invoice-app",
    "invoice-app:sourceMap": "rm -rf dist && nx build invoice-app --sourceMap",
    "designo:dev": "rm -rf dist && nx serve designo -- --port=4204 --proxy-config proxy.conf.json",
    "designo:build": "rm -rf dist && nx build designo",
    "designo:sourceMap": "rm -rf dist && nx build designo --sourceMap",
    "sandbox:dev": "rm -rf dist && nx serve sandbox -- --port=4205 --proxy-config proxy.conf.json",
    "sandbox:build": "rm -rf dist && nx build sandbox",
    "sandbox:sourceMap": "rm -rf dist && nx build sandbox --sourceMap",
    "frontendmentor:dev": "rm -rf dist && nx serve frontendmentor -- --port=4206 --proxy-config proxy.conf.json",
    "frontendmentor:build": "rm -rf dist && nx build frontendmentor",
    "frontendmentor:sourceMap": "nx build frontendmentor -- --sourceMap"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
